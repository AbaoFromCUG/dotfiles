{
    "author": "Abao Zhang",
    "name": "Qt QObject class",
    "options": [
        {
            "id": "class_name",
            "name": "class name",
            "default_value": "MyClass"
        }
    ],
    "output_files": [
        {
            "from": "qobject_h.templ",
            "to": "%{lua string.lower(class_name)}.h"
        },
        {
            "from": "qobject_cpp.templ",
            "to": "%{lua string.lower(class_name)}.cpp"
        }
    ]
}
