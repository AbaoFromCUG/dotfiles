{
    "author": "Abao Zhang",
    "name": "C++ class",
    "options": [
        {
            "id": "class_name",
            "name": "class name",
            "default_value": "AClass"
        }
    ],
    "output_files": [
        {
            "from": "class_h.templ",
            "to": "%{lua string.lower(class_name)}.h"
        },
        {
            "from": "class_cpp.templ",
            "to": "%{lua string.lower(class_name)}.cpp"
        }
    ]
}
