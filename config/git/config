[credential]
    helper = store

[commit]
    verbose = true

[core]
    autocrlf = input
	longpaths = true

[help]
    autocorrect = 20

[alias]
    changes = "log -M -L"

[push]
    autoSetupRemote = true

[pull]
    rebase = true

[merge]
    conflictstyle = diff3


[init]
    defaultBranch = master

[includeIf "hasconfig:remote.*.url:git@github.com:*/*"]
    path = github.inc
