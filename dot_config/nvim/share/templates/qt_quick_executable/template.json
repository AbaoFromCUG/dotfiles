{
    "author": "Abao Zhang",
    "name": "Qt Quick Application",
    "options": [
        {
            "id": "project_name",
            "name": "project name",
            "default_value": "app"
        },
        {
            "id": "target_name",
            "name": "target name",
            "default_value": "executable"
        }
    ],
    "output_files": [
        {
            "from": "CMakeLists_txt.templ",
            "to": "%{project_name}/CMakeLists.txt"
        },
        {
            "from": "main_cpp.templ",
            "to": "%{project_name}/main.cpp"
        },
        {
            "from": "main_qml.templ",
            "to": "%{project_name}/qml/main.qml"
        }
    ]
}
